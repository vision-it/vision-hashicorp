# This file is managed by Puppet

server = true
bootstrap_expect = <%= @retry_join.length %>
datacenter = "dc1"
encrypt = "<%= @encrypt.unwrap %>"
data_dir = "/opt/consul"
client_addr = "0.0.0.0"
advertise_addr = "<%= @advertise_addr %>"
retry_join = [<%= @retry_join.map{|s| "\"#{s}\""}.join(', ')%>]
log_level = "warn"
ui = false
