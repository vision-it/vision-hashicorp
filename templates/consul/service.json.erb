# This file is managed by Puppet

{
  "service": {
    "id": "<%= @title %>",
    "name": "<%= @service_name %>",
    "tags":  [<%= @tags.map{|s| "\"#{s}\""}.join(', ')%>],
    "address": "<%= @address %>",
    "port": <%= @port %>,
    "checks": [
    <% @checks.each do |check| -%>
    <%= check.to_json %>
    <% end -%>
 ]
}