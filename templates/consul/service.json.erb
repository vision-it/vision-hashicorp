{
  "service": {
    "_comment": "This file is managed by Puppet"
    "id": "<%= @service_id %>",
    "name": "<%= @service_name %>",
    "tags":  [<%= @tags.map{|s| "\"#{s}\""}.join(', ')%>],
    "address": "<%= @address %>",
    "port": <%= @port %>,
    "checks": [
    <% @checks.each do |check| -%>
    <%= check.to_json %>
    <% end -%>
   ]
 }
}